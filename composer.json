{
    "name": "fractal512/laravel-og-image",
    "description": "Open Graph image generator for Laravel based on Intervention Image - PHP image processing library",
    "keywords": [
        "laravel",
        "open graph",
        "og-image",
        "facebook image",
        "twitter image",
        "vk image"
    ],
    "homepage": "https://github.com/fractal512/laravel-og-image",
    "license": "MIT",
    "authors": [
        {
            "name": "Denys Vashchuk",
            "email": "fractal512.web.dev@gmail.com",
            "homepage": "https://github.com/fractal512",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "fractal512/og-image": "*",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "intervention/image": "^3.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Fractal512\\LaravelOpenGraphImage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fractal512\\LaravelOpenGraphImage\\OpenGraphImageServiceProvider"
            ]
        }
    }
}
